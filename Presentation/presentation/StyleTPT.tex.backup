\begin{frame}
 \begin{itemize}
 \item This fractal behavior is related directly to the quality of the Diophantine approximation.
 \item To increase the rate we should minimize the (\ref{quadratic_form}).
 \end{itemize}
 \pause
 \begin{block}{Analyzing}
 Following the method described in [Ordentlich \textit{et al.} 2012] 
 we aim to find the two successive minima $\lambda_1$ et $\lambda_2$ 
 of (\ref{quadratic_form}).
 \begin{itemize}
  \item It is shown by [Ordentlich \textit{et al. 2012}]:\\ 
  \begin{center}
   ${\lambda_1\lambda_2\approx\frac{1+g^2}{\SNR}}$
   \pause
  \end{center}
  \item Get high Sum-Rate when the second successive minima is small.
  \pause
  \item The 2 successive minima are the diagonal entries of $ G_{reduce}$.
  \end{itemize}
  \end{block}
  \end{frame}

%%%%%%%%%%%%%%%%%%%%%%%

\begin{frame}{How is it possible to improve the Sum-Rate ? (1/2)}

\begin{block}{Definition (Diophantine approximation) - [Cassels, 1957]}
Approximate a real number $\theta$, by a rational number $\frac{p}{q}$ when $q\not=0$. 
\begin{center}
 $q\left\Vert q\theta\right\Vert=\min_{p\in \mathbb{Z},q\in \mathbb{Z}^{*}}|q\theta-p|$
\end{center}
Then, for any $\theta$, there are infinitely many $p$ and $q$ such that 
\begin{center}
 $q\left\Vert q\theta\right\Vert < 5^{-\frac{1}{2}}$
\end{center}
\end{block}
\pause
\begin{theorem}
 Let $\theta$ be irrational. 
 Then there are infinitely many $q$ such that 
 $q \left\Vert q\theta\right\Vert < 5^{-\frac{1}{2}}$. 
 If $\theta$ is equivalent to $\phi$, then the constant $5^{-\frac{1}{2}}$ 
 cannot be replaced by any smaller number. 
\end{theorem}
\end{frame}


\begin{frame}{How is it possible to improve the Sum-Rate ? (2/2)}
\uncover<1->{
\begin{definition}
Let $\phi=\frac{1+\sqrt{5}}{2}$ be the Golden ratio. The conjugate of $\phi$ will 
be defined as $\bar\phi=\frac{1-\sqrt{5}}{2}$. A number $g$ is equivalent to $\phi$ if:
\begin{equation}
\label{Golden_Approximation}
g=\frac{a\phi+b}{c\phi+d}\,,\;(a, b, c, d)\in\mathbb{Z\,\mathrm{~and}\,\det\left[\begin{array}{lc}
\mathrm{a} & \mathrm{b}\\
\mathrm{c} & \mathrm{d}
\end{array}\right]}=\pm1 
\end{equation}
\end{definition}}
\uncover<2->{
\begin{block}{Proposition}
When $\SNR$ is large enough, 
and if $g$ is a number equivalent 
to the Golden Ratio, then the second 
minimum of $q(a_1,a_2)$ is small, in consequence the sum-rate of the interference channel will be high.}
\uncover<3->{\begin{itemize}
 \item The worst choice of ``\text{g}'' are rational numbers.}
\uncover<4->{
 \item The best choice for ``\textit{g}'' are numbers equivalent to the Golden Ratio.
\end{itemize}
\end{block}}
\end{frame}

\begin{frame}{Precoded Compute-and-Forward}

\uncover<1->{
\begin{itemize}
 \item To avoid deep fadings in interesting regimes when \underline{$\SNR$ goes to infinity}:
 \begin{itemize}
  \item Use ``\textit{n}'' different Time-Slots (TS).
  \item Precode at each TS the transmitted codewords by multiplying them, at the transmitters, by a real number $\eta$.
 \end{itemize}
\end{itemize}}

\uncover<2->{\begin{block}{Efficient precoding scheme}
For different TS, the efficient precoding scheme is to use a value of $\eta_i$ different 
of $\eta_j$ for $i\neq j$ such that, if $\eta_i\cdot g \in\Q$, then, $\eta_j\cdot g \notin\Q,\forall i\neq j$.}
\uncover<3->{
\begin{itemize}
 \item By using this strategy, only one time slot over $n$ will result in a small sum-rate, for all values of $g$.}
 \uncover<4->{
 \item The best choice of $\eta_i$ is \underline{the Golden Ratio or its equivalent numbers}. (Slightly greater than 1). 
\end{itemize}

\end{block}}

