\subsection{Compute-and-Forward Transform}


\begin{frame}{Computation rate}
\uncover<1->{
\vspace{-0.65cm}
 \begin{block}{Computation rate}
 The \underline{Computation Rate} is the maximal rate at which users can transmit codewords 
 to destinations, when interested in decoding reliably the 
 equation $\sum_{i}a_{i}\mathbf{x}_{i}$.
 \begin{equation}
 \label{Computation_rate}
 R(\mathbf{h},\mathbf{a})=\frac{1}{2}\log_2^{+}
 \left\{\left({\parallel \mathbf{a}\parallel^{2}}-
 \frac{\mathsf{SNR}(\mathbf{h}^{T}\mathbf{a})^{2}}
 {1+\mathsf{SNR}{\parallel \mathbf{h}\parallel^{2}}}\right)^{-1}\right\}
 \end{equation}
 Where $\log_{2}^{+}(x)\triangleq \max(\log_{2}(x),0)$.\\
 For each user:$~\mathbf{h}=[1,g],~g\in\R$ and $\mathbf{a}=[a_1,a_2]\hspace{0.1cm}\text{with}~a_1,~a_2\in\Z$.
 }
 \vspace{0.2cm}
 \uncover<2->{
 \begin{itemize}
  \item \textcolor{red}{How to find the vector $\mathbf{a}$ to maximize the Rate ?!}
 \end{itemize}
 }
 \uncover<3->{
 \begin{itemize}
  \item The simplified expression is:
  \begin{equation}
  \label{Computation_rate_form}
  R(\mathbf{h},\mathbf{a})=\frac{1}{2}\log_{2}^{+}
  \left\{ \frac{\left(\frac{1}{\mathsf{SNR}}+(1+g^{2})\right)}
  {q\left(a_1,a_2\right)}\right\} 
  \end{equation}
  \vspace{-0.2cm}
  Where $q(a_1,a_2)$ is a definite positive quadratic form equal to:
  \begin{equation}
  \label{quadratic_form}
  q\left(a_1,a_2\right)=(a_{1}g-a_2)^{2}+\frac{1}{\mathsf{SNR}}(a_{1}^{2}+a_{2}^{2}),~a_1,~a_2\in\mathbb{Z}
  \end{equation}
  \end{itemize}
 \end{block}}
\end{frame}

%%%%%%%%%%%%%%%%%%

\begin{frame}{Lattice $\Lambda_{CF}$, Maximizing the rate}
\uncover<1->{
\vspace{-0.3cm}
 \begin{block}{Lattice $\Lambda_{CF}$}
 \centering
 \begin{flushleft}{When $\SNR$ is finite the Gram matrix of definite positive 
 form (\ref{quadratic_form}) will define a rank 2 lattice $\Lambda_{CF}$.}
 \end{flushleft}
 \vspace{-0.3cm}
 \begin{equation}
 \label{Gram_Matrix}
  G= \left(\begin{array}{ c c } g^{2}+\frac{1}{\mathsf{SNR}} & -g \\ -g & 1+\frac{1}{\mathsf{SNR}} \end{array} \right)\
 \end{equation}
\end{block}}

