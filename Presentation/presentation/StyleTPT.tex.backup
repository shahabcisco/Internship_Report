\subsection{Lattice Definitions}

\begin{frame}
\frametitle{Lattice Definitions (1/2)}

\begin{columns}
\column{0.5\linewidth}
\vspace{-0.1cm}
\begin{block}{Lattice}<1-> 
\begin{itemize}
 \item Lattice is a discrete subgroup of Euclidean space.
 \centering{$\Lambda=\left\lbrace \mathbf{x}= \mathbf{M} \mathbf{s} , \mathbf{s} \in \mathbb{Z}^{n} \right\rbrace \subseteq \mathbb{R}^{n}$}\\ 
 \vspace{-0.15cm}
 \begin{flushleft}{$\mathbf{M}$: a \textcolor{blue}{\textit{generator matrix}}.\\ $\mathbf{G}=\mathbf{M}^{t}\mathbf{M}$: \textcolor{blue}{Gram matrix}.}
 \end{flushleft}
 \uncover<2->{\item Close under reflection \& addition.}
 \end{itemize}
\end{block}
\vspace{-0.1cm}
\begin{block}{Lattice Quantizer}<4-> For $\mathbf{x} \in \mathbb{R}^{n}$ returns the \textcolor{blue}{\textit{Closest Point} in $\Lambda$}: $\mathit{Q}_{\Lambda}(\mathbf{x}) = \argmin_{\lambda \in \Lambda} \parallel \mathbf{x} - \lambda \parallel$. 
\end{block}
\vspace{-0.1cm}
\begin{block}{Voronoi region}<7->
Set of points that quantize to a given lattice point $\lambda \in \Lambda$. The \textit{\textcolor{blue}{fundamental Voronoi Region}} $\mathcal{V}_{\Lambda}$ corresponds to the zero vector.
\end{block}

